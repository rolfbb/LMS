@model LMS.Models.HeaderViewModel

<header id="lmsheader">
    <div class="container body-content">
        <div class="row">          
            <div class="col-sm-4">Course: @Model.CourseName</div>
            <div class="col-sm-8">@Model.Description</div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                Start Date: @Html.DisplayFor(model => model.StartDate)
            </div>
        </div>
    </div>
</header>
